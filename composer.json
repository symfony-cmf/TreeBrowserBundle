{
    "name": "symfony-cmf/tree-browser-bundle",
    "type": "symfony-bundle",
    "description": "Symfony CMF Tree Browser Bundle",
    "keywords": [
        "Symfony CMF",
        "tree",
        "phpcr"
    ],
    "homepage": "http://cmf.symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/TreeBrowserBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony-cmf/resource-rest-bundle": "^1.0"
    },
    "require-dev": {
        "symfony/form": "^3.4 || ^4.1",
        "symfony-cmf/testing": "^2.1.8",
        "symfony/phpunit-bridge": "^4.2.2"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\TreeBrowserBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\TreeBrowserBundle\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
